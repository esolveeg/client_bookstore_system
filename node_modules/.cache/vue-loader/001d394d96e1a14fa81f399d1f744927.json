   return {\n            hasError:false,\n            errorMsg:\"\"\n        }\n    },\n    methods: {\n        update(val){\n            this.validation.forEach(cond => {\n            console.log(cond)\n            let errors = []\n\t\t\tfor (let condition of this.validation) {\n\t\t\t\tif (!condition.regex.test(val)) {\n\t\t\t\t\terrors.push(condition.message)\n\t\t\t\t}\n\t\t\t}\n\t\t\tif (errors.length === 0) {\n                 this.hasError = false\n                 this.errorMsg = \"\"\n\t\t\t\t\n\t\t\t} else {\n\t\t\t\tthis.hasError = true\n                this.errorMsg = errors[0]\n\t\t\t}\n                \n            });\n            this.$emit('input' , val)\n        }\n    }\n};\n</script>\n\n<style></style>\n"]}]}
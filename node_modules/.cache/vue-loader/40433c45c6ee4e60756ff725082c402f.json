n        @edit=\"edit\"\n      />\n      <div class=\"row\">\n        <div class=\"col s12 mt-65\">\n          <div class=\"container\">\n            <!-- Current balance & total transactions cards-->\n            <div class=\"row mt-4\">\n              <div class=\"col s12 m12 l12\">\n                <!-- Current Balance -->\n                <div class=\"card animated  fadeInUp \">\n                  <div class=\"card-content\">\n                    <div>\n                      <div class=\"item\">\n                        <h3 class=\"item__head\">\n                          {{ $t(\"name\") }}\n                        </h3>\n                        <span class=\"item__value\">\n                          {{ roleById.name }}\n                        </span>\n                      </div>\n                      <div class=\"item\">\n                        <h3 class=\"item__head\">\n                          {{ $t(\"description\") }}\n                        </h3>\n                        <span class=\"item__value\">\n                          {{ roleById.description }}\n                        </span>\n                      </div>\n                      <div class=\"item\">\n                        <h3 class=\"item__head\">\n                          {{ $t(\"permissions\") }}\n                        </h3>\n                        <span class=\"item__value\">\n                          <ul>\n                            <li\n                              v-for=\"(permission,\n                              index) in roleById.permissions\"\n                              :key=\"index\"\n                            >\n                              <router-link\n                                :to=\"{\n                                  name: 'permission.view',\n                                  params: { id: permission.id }\n                                }\"\n                                >{{ permission.name }}</router-link\n                              >\n                            </li>\n                          </ul>\n                        </span>\n                      </div>\n                      <div class=\"item\">\n                        <h3 class=\"item__head\">\n                          {{ $t(\"users\") }}\n                        </h3>\n                        <span class=\"item__value\">\n                          <ul>\n                            <li\n                              v-for=\"(user, index) in roleById.users\"\n                              :key=\"index\"\n                            >\n                              <router-link\n                                :to=\"{\n                                  name: 'users.view',\n                                  params: { id: user.id }\n                                }\"\n                                >{{ user.name }}</router-link\n                              >\n                            </li>\n                          </ul>\n                        </span>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport Breadcrumbs from \"@/components/layouts/Breadcrumbs.vue\";\nimport { roleQuery } from \"@/graphql/queries/queries.js\";\n\nexport default {\n  data() {\n    return {\n      title: \"View Role\",\n      roleById: {},\n      btns: [],\n      path: [{ title: \"roles\", route: \"roles.index\" }]\n    };\n  },\n\n  apollo: {\n    roleById: {\n      query: roleQuery,\n      variables() {\n        return {\n          id: parseInt(this.$route.params.id)\n        };\n      }\n    }\n  },\n  components: {\n    Breadcrumbs\n  }\n};\n</script>\n<style>\n@import url(\"../../sass/components/_recipt.css\");\n</style>\n<style lang=\"scss\" scoped>\n.item {\n  &__head {\n    text-transform: capitalize;\n    font-size: 1.8rem;\n    color: #37474f;\n    font-family: Open Sans, sans-serif;\n  }\n  &__value,\n  &__value ul li a {\n    font-family: Open Sans, sans-serif;\n    font-size: 1.4rem;\n    line-height: 1.57142857;\n    transition: 0.3s ease-out;\n    color: #76838f;\n    & ul li a:hover {\n      color: blue;\n    }\n  }\n}\n.item:not(:last-child) {\n  &::after {\n    content: \"\";\n    width: 100%;\n    height: 1px;\n    background-color: #eee;\n    display: block;\n    margin-top: 3rem;\n  }\n}\n</style>\n"]}]}
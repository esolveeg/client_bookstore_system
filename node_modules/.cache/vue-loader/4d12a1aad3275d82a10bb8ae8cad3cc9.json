gment\" />\n    <div class=\"col s12 mt-65\">\n      <div class=\"container\">\n        <!-- Current balance & total transactions cards-->\n        <div class=\"row mt-4\">\n          <div class=\"col s12 m12 l12\">\n            <!-- Current Balance -->\n            <h5 class=\"form-title\">\n              <i class=\"material-icons app-header-icon text-top\"\n                >perm_identity</i\n              >\n              {{ $t(title, { fregment }) }}\n            </h5>\n            <div class=\"card animated  fadeInLeft \">\n              <div class=\"card-content\">\n                <form @submit.prevent=\"submitted()\">\n                  <div class=\"card-alert danger card\" v-if=\"validationError\">\n                    <div class=\"card-content\">\n                      <p>\n                        <i class=\"material-icons\">error</i>{{ validationError }}\n                      </p>\n                    </div>\n                  </div>\n                  <div class=\"row\">\n                    <div\n                      class=\"input-field col s12\"\n                      :class=\"{ error: validation.hasError('name') }\"\n                    >\n                      <input v-model=\"name\" id=\"name\" type=\"text\" />\n                      <label for=\"name\" :class=\"name ? 'active' : ''\">{{\n                        $t(\"name\")\n                      }}</label>\n                      <div class=\"message\">\n                        {{ validation.firstError(\"name\") }}\n                      </div>\n                    </div>\n                  </div>\n                  <div class=\"row\">\n                    <div\n                      class=\"input-field col s12\"\n                      :class=\"{ error: validation.hasError('email') }\"\n                    >\n                      <input v-model=\"email\" id=\"email\" type=\"email\" />\n                      <label for=\"email\" :class=\"email ? 'active' : ''\">{{\n                        $t(\"email\")\n                      }}</label>\n                      <div class=\"message\">\n                        {{ validation.firstError(\"email\") }}\n                      </div>\n                    </div>\n                  </div>\n                  <div class=\"row\">\n                    <div\n                      class=\"input-field col s12\"\n                      :class=\"{ error: validation.hasError('phone') }\"\n                    >\n                      <input v-model=\"phone\" id=\"phone\" type=\"text\" />\n                      <label for=\"phone\" :class=\"phone ? 'active' : ''\">{{\n                        $t(\"phone\")\n                      }}</label>\n                      <div class=\"message\">\n                        {{ validation.firstError(\"phone\") }}\n                      </div>\n                    </div>\n                  </div>\n                  <div class=\"row\">\n                    <div\n                      class=\"input-field col s12\"\n                      :class=\"{ error: validation.hasError('intersts') }\"\n                    >\n                      <textarea\n                        v-model=\"intersts\"\n                        id=\"intersts\"\n                        class=\"materialize-textarea\"\n                      ></textarea>\n                      <label for=\"intersts\" :class=\"intersts ? 'active' : ''\">{{\n                        $t(\"intersts\")\n                      }}</label>\n                      <div class=\"message\">\n                        {{ validation.firstError(\"intersts\") }}\n                      </div>\n                    </div>\n                  </div>\n\n                  <div class=\"row\" v-if=\"!isPos\">\n                    <div\n                      class=\"input-field col s12\"\n                      :class=\"{ error: validation.hasError('branch') }\"\n                    >\n                      <select id=\"branch\" v-model=\"branch\">\n                        <option value=\"\" disabled=\"none\">{{\n                          $t(\"select_branch\")\n                        }}</option>\n                        <option\n                          v-for=\"branch in allBranches\"\n                          :key=\"branch.id\"\n                          :value=\"branch.id\"\n                          >{{ branch.name }}</option\n                        >\n                      </select>\n                      <label for=\"branch\" :class=\"branchs ? 'active' : ''\">{{\n                        $t(\"branches\")\n                      }}</label>\n                      <div class=\"message\">\n                        {{ validation.firstError(\"users\") }}\n                      </div>\n                    </div>\n                  </div>\n                  <div class=\"row\">\n                    <div class=\"input-field col s12\">\n                      <button\n                        class=\"btn create cyan waves-effect waves-light left\"\n                        type=\"submit\"\n                        name=\"action\"\n                        disabled=\"disabled\"\n                      >\n                        {{ $t(\"save\") }}\n                        <i class=\"material-icons right\">send</i>\n                      </button>\n                    </div>\n                  </div>\n                </form>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport CustomersMixin from \"@/mixins/EditAddCustomersMixin.js\";\n\nexport default {\n  name: \"edit-add-Customers\",\n  mixins: [CustomersMixin]\n};\n</script>\n<style lang=\"scss\" scoped>\n.form-title {\n  font-weight: 700;\n  line-height: 3.6rem;\n  font-size: 2.64rem;\n  margin-bottom: 5rem;\n  & i {\n    font-size: 3.6rem;\n    vertical-align: bottom;\n  }\n}\n\nlabel {\n  font-size: 1.6rem !important;\n}\ninput,\n.checkbox span:not(.lever),\ntextarea,\nselect {\n  font-size: 1.6rem !important;\n  height: 4.2rem !important;\n}\n.checkbox {\n  position: relative;\n}\n</style>\n"]}]}
    <p>\n                      <strong>rest</strong>:<span>{{ posDataD.rest }}EGP</span>\n                    </p>\n                    <p>\n                      <strong>over</strong>:<span>{{ posDataD.over }}EGP</span>\n                    </p>\n                  </div>\n                  <div class=\"tableFilters\">\n                    <div class=\"input-field\" v-if=\"isPos == false\">\n                      <select id=\"branches\" v-model=\"branchId\">\n                        <option value=\"\" disabled=\"none\">{{\n                          $t(\"branches\")\n                        }}</option>\n                        <option value=\"\">{{ $t(\"none\") }}</option>\n                        <option value=\"store\">{{ $t(\"store\") }}</option>\n                        <option\n                          :value=\"branch.id\"\n                          v-for=\"branch in allBranches\"\n                          :key=\"branch.id\"\n                          >{{ branch.name }}</option\n                        >\n                      </select>\n                      <label for=\"branches\">{{ $t(\"branches\") }}</label>\n                    </div>\n                    <div class=\"input-field\" v-if=\"isPos == false\">\n                      <select id=\"users\" v-model=\"userIdFilter\">\n                        <option value=\"\" disabled=\"none\">{{\n                          $t(\"users\")\n                        }}</option>\n                        <option value=\"store\">{{ $t(\"none\") }}</option>\n                        <option\n                          :value=\"user.id\"\n                          v-for=\"user in allUsers\"\n                          :key=\"user.id\"\n                          >{{ user.name }}</option\n                        >\n                      </select>\n                      <label for=\"users\">{{ $t(\"users\") }}</label>\n                    </div>\n                    <div class=\"input-field\">\n                      <label for=\"branches\">{{ $t(\"from\") }}</label>\n                      <input type=\"date\" v-model=\"dateFrom\" />\n                    </div>\n                    <div class=\"input-field\">\n                      <label for=\"branches\">{{ $t(\"to\") }}</label>\n                      <input type=\"date\" v-model=\"dateTo\" />\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n          <div class=\"row mt-4\">\n            <div class=\"col s12 m12 l12\">\n              <!-- Current Balance -->\n              <DataTable\n                :data=\"ordersQuery\"\n                :canDelete=\"canDelete\"\n                :canView=\"canView\"\n                :canEdit=\"canEdit\"\n                :columns=\"columns\"\n                :sorting=\"sorting\"\n                @setDeleteData=\"setDeleteData\"\n                @goToEdit=\"goToEdit\"\n                @goToView=\"goToView\"\n              >\n              </DataTable>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport OrdersMixin from \"@/mixins/BrowseOrdersMixin.js\";\n\nexport default {\n  name: \"browseOrders\",\n  mixins: [OrdersMixin]\n};\n</script>\n<style lang=\"scss\">\n.filter-card {\n  z-index: 2;\n}\n.tableFilters {\n  display: flex;\n\n  @media screen AND (max-width: 768px) {\n    display: block;\n  }\n  .input-field {\n    margin-right: 3rem;\n    @media screen AND (max-width: 768px) {\n      margin-bottom: 3rem;\n      margin-right: 0;\n    }\n  }\n\n  & label {\n    font-size: 1.4rem;\n  }\n}\n</style>\n"]}]}
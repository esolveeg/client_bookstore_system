AACA,UAAIC,QAAQ,GAAG7D,CAAC,CAAC8D,KAAF,CAAQC,WAAR,CAAoBL,IAApB,CAAf;AACAG,MAAAA,QAAQ,CAACG,IAAT;AACD;AApCM,GAxGI;AA8IbU,EAAAA,UAAU,EAAE;AACV5E,IAAAA,SAAS,EAATA,SADU;AAEVC,IAAAA,WAAW,EAAXA;AAFU;AA9IC,CAAf","sourcesContent":["import DataTable from \"@/components/datatable/Table.vue\";\nimport Breadcrumbs from \"@/components/layouts/Breadcrumbs.vue\";\nimport M from \"materialize-css\";\nimport { transactionsQuery, meQuery } from \"@/graphql/queries/queries.js\";\nexport default {\n  data() {\n    let columns = [\n      { label: \"approved\", name: \"approved\", dbname: \"approved\" },\n      { label: \"branch\", name: \"branch\", dbname: \"branch\", sub: \"name\" },\n      { label: \"created_at\", name: \"created_at\", dbname: \"created_at\" }\n    ];\n    let sorting = [{ field: \"created_at\", order: \"ASC\" }];\n    return {\n      columns,\n      sorting,\n      transactions: [],\n      title: \"transactions\",\n      canAdd: false,\n      canApprove: true,\n      canDelete: false,\n      canView: false,\n      canEdit: false,\n      btns: []\n    };\n  },\n  apollo: {\n    me: {\n      query: meQuery,\n      result({ data: { me } }) {\n        let permissions = [];\n        me.roles.forEach(role => {\n          role.permissions.forEach(per => {\n            permissions.push(per.slug);\n          });\n        });\n        permissions.includes(\"create_transactions\") ? (this.canAdd = true) : \"\";\n        permissions.includes(\"delete_transactions\")\n          ? (this.canDelete = true)\n          : \"\";\n        permissions.includes(\"edit_transactions\") ? (this.canEdit = true) : \"\";\n        permissions.includes(\"read_transactions\") ? (this.canView = true) : \"\";\n      }\n    },\n\n    transactions: {\n      query: transactionsQuery,\n      // fetchPolicy: 'cache-and-network',\n      variables() {\n        return this.branch\n          ? {\n              first: this.tableData.length,\n              page: this.page,\n              orderBy: this.sorting,\n              branch: this.branch,\n              search: this.tableData.search\n            }\n          : {\n              first: this.tableData.length,\n              page: this.page,\n              orderBy: this.sorting,\n              search: this.tableData.search\n            };\n      },\n      deep: true,\n      pollInterval: 1000\n    }\n  },\n  watch: {\n    ids: {\n      handler(value) {\n        let index = this.btns.indexOf(this.deleteBtn);\n        value.length > 0 && this.canDelete\n          ? this.btns.includes(this.deleteBtn)\n            ? \"\"\n            : this.btns.push(this.deleteBtn)\n          : index > -1\n          ? this.btns.splice(index, 1)\n          : \"\";\n      }\n    },\n    canAdd: {\n      handler(value) {\n        value ? this.btns.push(this.createBtn) : \"\";\n      }\n    },\n    canDelete: {\n      handler(value) {\n        value\n          ? this.ids.length > 0\n            ? this.btns.push(this.deleteBtn)\n            : \"\"\n          : \"\";\n      }\n    }\n  },\n  store: {\n    model: \"deleted_model\",\n    ids: \"selected_ids\",\n    me: \"me\",\n    branch: \"branch_id\",\n    deleteBtn: \"deleteBtn\",\n    createBtn: \"createBtn\",\n    perPage: \"perPage\",\n    page: \"current_page\",\n    deletedId: \"deleted_id\",\n    deletedName: \"deleted_name\",\n    tableData: \"tableData\"\n  },\n  methods: {\n    bulkdelete() {\n      this.model = \"transactions\";\n      let elem = document.querySelector(\"#multi-delete-modal\");\n      let instance = M.Modal.getInstance(elem);\n      instance.open();\n      // console.log(\"deletg\");\n    },\n    create() {\n      this.$router.push({ name: \"transactions.create\" });\n    },\n    goToView(id) {\n      this.$router.push({\n        name: \"transactions.view\",\n        params: { id: parseInt(id) }\n      });\n    },\n    goToApprove(id) {\n      this.$router.push({\n        name: \"transactions.approve\",\n        params: { id: parseInt(id) }\n      });\n    },\n    goToEdit(id) {\n      this.$router.push({\n        name: \"transactions.edit\",\n        params: { id: parseInt(id) }\n      });\n    },\n    setDeleteData(data) {\n      this.deletedName = data.name;\n      this.deletedId = data.id;\n      this.model = \"transactions\";\n      let elem = document.querySelector(\"#delete-modal\");\n      let instance = M.Modal.getInstance(elem);\n      instance.open();\n    }\n  },\n  components: {\n    DataTable,\n    Breadcrumbs\n  }\n};\n"]}]}
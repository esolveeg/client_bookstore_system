AAd;AAFQ,OAAlB;AAID,KAvBM;AAwBPI,IAAAA,aAxBO,yBAwBOlE,IAxBP,EAwBa;AAClB,WAAKiD,WAAL,GAAmBjD,IAAI,CAACG,IAAxB;AACA,WAAK6C,SAAL,GAAiBhD,IAAI,CAAC8D,EAAtB;AACA,WAAKhB,KAAL,GAAa,WAAb;AACA,UAAIM,IAAI,GAAGC,QAAQ,CAACC,aAAT,CAAuB,eAAvB,CAAX;AACA,UAAIC,QAAQ,GAAG1D,CAAC,CAAC2D,KAAF,CAAQC,WAAR,CAAoBL,IAApB,CAAf;AACAG,MAAAA,QAAQ,CAACG,IAAT;AACD;AA/BM,GA7FI;AA8HbS,EAAAA,UAAU,EAAE;AACVxE,IAAAA,SAAS,EAATA,SADU;AAEVC,IAAAA,WAAW,EAAXA;AAFU;AA9HC,CAAf","sourcesContent":["import DataTable from \"@/components/datatable/Table.vue\";\nimport Breadcrumbs from \"@/components/layouts/Breadcrumbs.vue\";\nimport M from \"materialize-css\";\nimport { suppliersQuery, meQuery } from \"@/graphql/queries/queries.js\";\nexport default {\n  data() {\n    let columns = [\n      { label: \"name\", name: \"name\", dbname: \"name\" },\n      { label: \"email\", name: \"email\", dbname: \"email\" },\n      { label: \"phone\", name: \"phone\", dbname: \"phone\" },\n      { label: \"created_at\", name: \"created_at\", dbname: \"created_at\" }\n    ];\n    let sorting = [{ field: \"created_at\", order: \"ASC\" }];\n    return {\n      columns,\n      sorting,\n      suppliers: [],\n      title: \"suppliers\",\n      canAdd: false,\n      canDelete: false,\n      canView: false,\n      canEdit: false,\n      btns: []\n    };\n  },\n  apollo: {\n    me: {\n      query: meQuery,\n      result({ data: { me } }) {\n        let permissions = [];\n        me.roles.forEach(role => {\n          role.permissions.forEach(per => {\n            permissions.push(per.slug);\n          });\n        });\n        permissions.includes(\"create_suppliers\") ? (this.canAdd = true) : \"\";\n        permissions.includes(\"delete_suppliers\") ? (this.canDelete = true) : \"\";\n        permissions.includes(\"edit_suppliers\") ? (this.canEdit = true) : \"\";\n        permissions.includes(\"read_suppliers\") ? (this.canView = true) : \"\";\n      }\n    },\n\n    suppliers: {\n      query: suppliersQuery,\n      // fetchPolicy: 'cache-and-network',\n      variables() {\n        return {\n          first: this.tableData.length,\n          page: this.page,\n          orderBy: this.sorting,\n          search: this.tableData.search\n        };\n      },\n      deep: true,\n      pollInterval: 1000\n    }\n  },\n  watch: {\n    ids: {\n      handler(value) {\n        let index = this.btns.indexOf(this.deleteBtn);\n        value.length > 0 && this.canDelete\n          ? this.btns.includes(this.deleteBtn)\n            ? \"\"\n            : this.btns.push(this.deleteBtn)\n          : index > -1\n          ? this.btns.splice(index, 1)\n          : \"\";\n      }\n    },\n    canAdd: {\n      handler(value) {\n        value ? this.btns.push(this.createBtn) : \"\";\n      }\n    },\n    canDelete: {\n      handler(value) {\n        value\n          ? this.ids.length > 0\n            ? this.btns.push(this.deleteBtn)\n            : \"\"\n          : \"\";\n      }\n    }\n  },\n  store: {\n    model: \"deleted_model\",\n    ids: \"selected_ids\",\n    me: \"me\",\n    deleteBtn: \"deleteBtn\",\n    createBtn: \"createBtn\",\n    perPage: \"perPage\",\n    page: \"current_page\",\n    deletedId: \"deleted_id\",\n    deletedName: \"deleted_name\",\n    tableData: \"tableData\"\n  },\n  methods: {\n    bulkdelete() {\n      this.model = \"suppliers\";\n      let elem = document.querySelector(\"#multi-delete-modal\");\n      let instance = M.Modal.getInstance(elem);\n      instance.open();\n\n      // console.log(\"deletg\");\n    },\n    create() {\n      this.$router.push({ name: \"suppliers.create\" });\n    },\n    goToView(id) {\n      this.$router.push({\n        name: \"suppliers.view\",\n        params: { id: parseInt(id) }\n      });\n    },\n    goToEdit(id) {\n      this.$router.push({\n        name: \"suppliers.edit\",\n        params: { id: parseInt(id) }\n      });\n    },\n    setDeleteData(data) {\n      this.deletedName = data.name;\n      this.deletedId = data.id;\n      this.model = \"suppliers\";\n      let elem = document.querySelector(\"#delete-modal\");\n      let instance = M.Modal.getInstance(elem);\n      instance.open();\n    }\n  },\n  components: {\n    DataTable,\n    Breadcrumbs\n  }\n};\n"]}]}